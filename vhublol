local Notification = loadstring(game:HttpGet("https://api.irisapp.ca/Scripts/IrisBetterNotifications.lua"))()

getgenv()["IrisAd"]=true

spawn(function()
  Notification.Notify("V-Hub", "Loading! Please Wait...", "rbxassetid://8577247587");
end)

wait(5)

